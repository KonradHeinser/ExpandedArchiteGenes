    <Operation Class="PatchOperationConditional">
        <xpath>Defs/GeneDef[defName="EBSG_ResistantStomach"]</xpath>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class='PatchOperationAdd'>
                    <xpath>Defs</xpath>
                    <value>
                        <GeneDef>
                            <defName>EBSG_ArchiteDigestion</defName>
                            <label>archite enhanced digestion</label>
                            <description>Carriers of this gene have archites in their stomach that protect them from food poisoning and process raw foods with unbelievable efficiency.</description>
                            <iconPath>UI/Icons/Genes/Gene_RobustDigestion</iconPath>
                            <foodPoisoningChanceFactor>0</foodPoisoningChanceFactor>
                            <dontMindRawFood>true</dontMindRawFood>
                            <biostatArc>1</biostatArc>
                            <biostatCpx>3</biostatCpx>
                            <displayCategory>EBSG_Food</displayCategory>
                            <displayOrderInCategory>10.1</displayOrderInCategory>
                            <marketValueFactor>1.5</marketValueFactor>
                            <statFactors>
                                <RawNutritionFactor>2.0</RawNutritionFactor>
                            </statFactors>
                            <symbolPack>
                                <prefixSymbols>
                                    <li><symbol>dura</symbol></li>
                                    <li><symbol>eat</symbol></li>
                                    <li><symbol>feed</symbol></li>
                                    <li><symbol>cram</symbol></li>
                                </prefixSymbols>
                                <suffixSymbols>
                                    <li><symbol>vore</symbol></li>
                                </suffixSymbols>
                            </symbolPack>
                            <exclusionTags>
                                <li>MBSG_Digestion</li>
                                <li>EBSG_StrongStomachs</li>
                            </exclusionTags>
                        </GeneDef>
                    </value>
                </li>

                <li Class="PatchOperationAdd">
                    <xpath>Defs/ThoughtDef[defName="AteRawFood"]/nullifyingGenes</xpath>
                    <value>
                        <li>EBSG_ArchiteDigestion</li>
                    </value>
                </li>

                <li Class="PatchOperationConditional">
                    <xpath>Defs/GeneCategoryDef[defName="AG_FoodConsumption"]</xpath>
                    <match Class="PatchOperationReplace">
                        <xpath>Defs/GeneDef[defName="EBSG_ArchiteDigestion"]/displayCategory</xpath>
                        <value><displayCategory>AG_FoodConsumption</displayCategory></value>
                    </match>
                </li>
            </operations>
        </match>
    </Operation>

